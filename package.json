{
    "name": "jsmvcfw",
    "version": "1.0.0",
    "description": "Javascript mvc framework",
    "author": "cimo",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/cimo/JsMvcFw.git"
    },
    "scripts": {
        "preinstall": "npm install --package-lock-only --ignore-scripts --no-audit && npx --yes npm-force-resolutions@0.0.3",
        "check": "eslint . --fix",
        "build": "npm run check && tsc --build -clean && tsc --build tsconfig.build.json"
    },
    "dependencies": {},
    "devDependencies": {
        "@types/node": "18.14.4",
        "@typescript-eslint/parser": "5.54.0",
        "@typescript-eslint/eslint-plugin": "5.54.0",
        "eslint": "8.35.0",
        "eslint-plugin-prettier": "4.2.1",
        "eslint-config-prettier": "8.8.0",
        "prettier": "2.8.7",
        "typescript": "4.9.4",
        "tslib": "2.5.0"
    },
    "resolutions": {
        "node-fetch": "2.6.7",
        "**/node-fetch": "2.6.7"
    },
    "files": [
        "./dist/"
    ],
    "types": "./dist/index.d.ts",
    "keywords": [
        "mvc",
        "mvc framework",
        "nodejs",
        "typescript",
        "cimo"
    ]
}
